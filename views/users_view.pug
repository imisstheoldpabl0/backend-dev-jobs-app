doctype html
html(lang="en")
  head
    meta(charset="UTF-8")
    meta(name="viewport" content="width=device-width, initial-scale=1.0")
    link(rel="stylesheet" href="./style.css")
    title users
  body 
  include ./header.pug
  main(id="users_main")
    h1 All users
    each user in data
      section(id="user_card")
        ul
          li #{user.name} #{user.surname}
          li #{user.email}
  script(src="./script.js")