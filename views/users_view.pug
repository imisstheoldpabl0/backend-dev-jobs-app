doctype html
html(lang="en")
  head
    meta(charset="UTF-8")
    meta(name="viewport", content="width=device-width, initial-scale=1.0")
    link(rel="stylesheet", href="./style.css")
    title users
  body 
    include ./header.pug
    main
      h1 All users
        each user in data
          ul 
            li e-mail: #{user.name}
            li user: #{user.surname}
            li password: #{user.email}
    script(src="./script.js")